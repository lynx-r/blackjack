         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" />
         </rowConstraints>
         <children>
            <Label fx:id="dealerScoreLabel" maxWidth="1.7976931348623157E308" text="Dealer score: 0" GridPane.hgrow="NEVER" GridPane.valignment="TOP">
               <padding>
                  <Insets right="4.0" />
               </padding>
            </Label>
            <HBox fx:id="dealerCardsHBox" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1" />
