               <padding>
                  <Insets right="4.0" />
               </padding>
            </Label>
            <HBox fx:id="playerCardsHBox" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
