<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="310.0" prefWidth="521.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.blackjack.controller.MainWindowController">
   <children>
      <GridPane VBox.vgrow="ALWAYS">
